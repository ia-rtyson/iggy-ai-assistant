{
  "custom": {
    "componentConfig": null,
    "data": []
  },
  "params": {
    "item_id": ""
  },
  "propConfig": {
    "custom.componentConfig": {
      "binding": {
        "config": {
          "path": "view.custom.data[0][\"data\"]"
        },
        "transforms": [
          {
            "code": "\ttry:\n\t\tconfig \u003d system.util.jsonDecode(value)\n\t\treturn config\n\texcept:\n\t\treturn {}\n\t\n\t",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "custom.data": {
      "binding": {
        "config": {
          "enableValueCache": false,
          "parameters": {
            "id": "{view.params.item_id}"
          },
          "queryPath": "Exchange/Iggy/Artifact/select_artifact",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\t\n\treturn value",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "params.item_id": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 435,
      "width": 481
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.custom.data[0].description"
                  },
                  "overlayOptOut": true,
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "classes": "component/title"
              }
            },
            "type": "ia.display.label"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "EmbeddedView"
                },
                "position": {
                  "basis": "68px",
                  "display": false,
                  "shrink": 0
                },
                "propConfig": {
                  "props.params.toCopy": {
                    "binding": {
                      "config": {
                        "path": "view.custom.data[0].data"
                      },
                      "transforms": [
                        {
                          "code": "\treturn str(value.replace(\"```python\",\u0027\u0027))",
                          "type": "script"
                        }
                      ],
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "path": "Widgets/Copy To Clipboard"
                },
                "type": "ia.display.view"
              }
            ],
            "meta": {
              "name": "FlexContainer"
            },
            "position": {
              "basis": "200px"
            },
            "props": {
              "justify": "flex-end"
            },
            "type": "ia.container.flex"
          }
        ],
        "meta": {
          "name": "FlexContainer_header"
        },
        "position": {
          "basis": "50px",
          "shrink": 0
        },
        "props": {
          "justify": "space-between",
          "style": {
            "borderBottomColor": "--neutral-20",
            "borderBottomStyle": "solid",
            "borderBottomWidth": "3px",
            "paddingLeft": "20px"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "meta": {
          "name": "Image"
        },
        "position": {
          "basis": "250px"
        },
        "propConfig": {
          "props.source": {
            "binding": {
              "config": {
                "path": "view.custom.componentConfig"
              },
              "transforms": [
                {
                  "code": "\tif value \u003d\u003d None:\n\t\treturn \"\"\n\tif \"source\" in self.view.custom.componentConfig:\n\t\treturn self.view.custom.componentConfig[\"source\"]\n\treturn \"data:image/png;base64,%s\" % value",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "fit": {
            "mode": "contain"
          },
          "style": {
            "paddingBottom": "20px",
            "paddingLeft": "20px",
            "paddingRight": "20px",
            "paddingTop": "10px"
          }
        },
        "type": "ia.display.image"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "artifact/root"
      }
    },
    "type": "ia.container.flex"
  }
}
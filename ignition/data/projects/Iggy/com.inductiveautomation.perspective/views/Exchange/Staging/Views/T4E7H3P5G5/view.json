              "name": "small",
              "span": 12,
              "rowIndex": 4,
              "colIndex": 0
            }
          ]
        },
        "props": {
          "label": {
            "text": "Country"
          },
          "placeholder": {
            "text": "Select a country"
          },
          "options": {
            "binding": {
              "type": "http",
              "config": {
                "request": {
                  "url": "https://restcountries.com/v3.1/all?fields=name",
                  "method": "GET"
                }
              },
              "transforms": [
                {
                  "type": "script",
                  "code": "countries = []
for country in value:
    countries.append({
        "value": country["name"]["common"],
        "text": country["name"]["common"]
    })
countries = sorted(countries, key=lambda c: c['text'])
return countries"
                }
              ]
            }
          },
          "validation": {
            "constraints": {
              "required": {
                "enabled": true
              }
            }
          }
        },
        "type": "ia.input.dropdown"

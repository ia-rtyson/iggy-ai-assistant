              {
                "rows": {
                  "items": [
                    {
                      "widgets": [
                        {
                          "id": "datepicker-widget",
                          "label": {
                            "text": "Date Picker"
                          },
                          "type": "date-picker"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "timepicker-widget",
                          "label": {
                            "text": "Time Picker"
                          },
                          "type": "time-picker"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "datetimepicker-widget",
                          "label": {
                            "text": "Date/Time Picker"

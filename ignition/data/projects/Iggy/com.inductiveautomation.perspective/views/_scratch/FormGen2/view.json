{
  "custom": {},
  "params": {},
  "props": {},
  "root": {
    "children": [
      {
        "meta": {
          "name": "Form"
        },
        "position": {
          "basis": "400px",
          "grow": 1
        },
        "props": {
          "actions": {
            "justify": "end",
            "submit": {
              "fireSubmissionEvent": true,
              "text": "Submit Feedback"
            }
          },
          "columns": {
            "align": "stretch",
            "items": [
              {
                "rows": {
                  "align": "start",
                  "items": [
                    {
                      "widgets": [
                        {
                          "id": "text-widget",
                          "label": {
                            "text": "Text"
                          },
                          "text": {
                            "autoComplete": "off",
                            "placeholder": "",
                            "style": {
                              "classes": ""
                            },
                            "validation": {
                              "constraints": {
                                "maxLength": {
                                  "enabled": false,
                                  "value": null
                                },
                                "minLength": {
                                  "enabled": false,
                                  "value": null
                                },
                                "pattern": {
                                  "enabled": false,
                                  "value": ""
                                },
                                "required": {
                                  "enabled": false
                                }
                              },
                              "icon": {
                                "color": "",
                                "path": "",
                                "style": {}
                              },
                              "message": {},
                              "style": {
                                "classes": ""
                              }
                            }
                          },
                          "type": "text"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "password-widget",
                          "label": {
                            "text": "Password"
                          },
                          "type": "password"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "number-widget",
                          "label": {
                            "text": "Number"
                          },
                          "type": "number"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "tel-widget",
                          "label": {
                            "text": "Telephone"
                          },
                          "type": "tel"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "email-widget",
                          "label": {
                            "text": "Email"
                          },
                          "type": "email"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "url-widget",
                          "label": {
                            "text": "URL"
                          },
                          "type": "url"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "textarea-widget",
                          "label": {
                            "text": "Text Area"
                          },
                          "type": "text-area"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "slider-widget",
                          "label": {
                            "text": "Slider"
                          },
                          "type": "slider"
                        }
                      ]
                    }
                  ],
                  "justify": "start",
                  "style": {
                    "classes": ""
                  }
                },
                "style": {
                  "classes": ""

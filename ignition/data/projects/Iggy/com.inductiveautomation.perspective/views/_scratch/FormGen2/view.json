                          "radio": {
                            "options": [
                              {
                                "text": "Choice A",
                                "value": "A"
                              },
                              {
                                "text": "Choice B",
                                "value": "B"
                              }
                            ]
                          },
                          "type": "radio"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "checkbox": {
                            "options": [
                              {
                                "text": "Item X",
                                "value": "X"
                              },
                              {
                                "text": "Item Y",
                                "value": "Y"
                              }
                            ],
                            "type": "string"
                          },
                          "id": "checkbox-widget",
                          "label": {
                            "text": "Checkbox Group"
                          },
                          "type": "checkbox"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "toggle-widget",
                          "label": {
                            "text": "Toggle Switch"
                          },
                          "toggle": {
                            "options": [
                              {
                                "text": "On",
                                "value": "on"
                              },
                              {
                                "text": "Off",
                                "value": "off"
                              }
                            ],
                            "type": "string"
                          },
                          "type": "toggle"
                        }
                      ]
                    },
                    {
                      "widgets": [
                        {
                          "id": "info-widget",
                          "info": {
                            "text": "This is a widget of type \u0027none\u0027, used here to display an informational message.",
                            "type": "caption"
                          },
                          "label": {
                            "text": "Informational Widget"
                          },
                          "type": "none"
                        }
                      ]
                    }
                  ]
                }
              }
            ]
          },
          "name": "UIFeedbackForm",
          "style": {
            "classes": "p-4"
          }
        },
        "type": "ia.input.form"
